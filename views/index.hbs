<h1>Browser</h1>
  <form action="/" method="GET">
    <input type="text" class="search" name="search" placeholder="Search...">
    <input type="number" name="from" class="difficulty" placeholder="Price from...">
    <span>-</span>
    <input type="number" name="to" class="difficulty" placeholder="Price to...">
    <input type="submit" value="search">
  </form>
  {{#each cubes}}
  {{> cubePreview}}
  {{else}}
  <p>No cubes in the database</p>
  {{/each}}
  